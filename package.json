    "nuxt-jest-puppeteer": "^0.3.8"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "env": {
      "test": {
        "presets": [
          [
            "env",
